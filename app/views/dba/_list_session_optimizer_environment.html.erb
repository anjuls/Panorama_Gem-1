<%
column_options =
[
    {:caption=>'Name',          :data=>proc{|rec| rec.name },           :title=>'Name of parameter'},
    {:caption=>'SQL Feature',   :data=>proc{|rec| rec.sql_feature },    :title=>'Associated SQL feature'},
    {:caption=>'Default',       :data=>proc{|rec| rec.isdefault },      :title=>'Indicates whether the parameter is set to the default value (YES) or not (NO)'},
    {:caption=>'Value',         :data=>proc{|rec| rec.value },          :title=>'Value of the parameter for the session'},
]

%>
<%= gen_slickgrid(@env, column_options, {
                                :caption => "Optimizer environment for instance=#{@instance}, SID=#{@sid}",
                                :width=>:auto
                        }) %>
